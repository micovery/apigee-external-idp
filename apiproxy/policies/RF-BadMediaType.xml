<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-BadMediaType">
    <DisplayName>RF-BadMediaType</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <StatusCode>415</StatusCode>
            <ReasonPhrase>Unsupported Media Type</ReasonPhrase>
            <Payload contentType="application/json">
{
  "error": "BadMediaType",
  "message": "OAuth endpoint only accepts \"application/x-www-form-urlencoded\" for POST requests."
}
            </Payload>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>